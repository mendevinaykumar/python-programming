l1 = []
l2 = []
l3 = []

n1 = int(input("enter the size of list"))
n2 = int(input("enter the size of list"))
n3 = int(input("enter the size of list"))

def createlist(l,n):
    for i in range(n):
        temp = int(input("enter the element in the list"))
        l.append(temp)
        for i in l:
            print(i)
        return(l)
l1=createlist(l1,n1)
l2=createlist(l2,n2)
l3=createlist(l3,n3)
